/* searchMore.css */
page {
    background: #eee;
}
.from-wrapper {
    padding: 10pt;
}
.from-wrapper .input-box {
    background: #fff;
    display: flex;
    align-items: center;
    margin-bottom: 10pt;
}
.from-wrapper .input-box input{
    flex: 1;
    height: 30pt;
    font-size: 12pt;
    padding: 0 10pt;
}
.from-wrapper .input-box .btn {
    width: 40pt;
    background: #168FED;
    height: 30pt;
    line-height: 30pt;
    text-align: center;
    color: #fff;
}
.location {
    position: absolute;
    top: 90pt;
    bottom: 0;
    width: 100%;
}
.location .location-item {
    background: #fff;
    padding: 5pt 10pt;
    border-bottom: 1pt solid #ddd;
}
.location .location-item .title {
    display: flex;
}
.location .location-item>view {
    color: #999;
    font-size: 10pt;
}
.location .location-item .title>view:first-child {
    flex: 1;
    color: #333;
}
